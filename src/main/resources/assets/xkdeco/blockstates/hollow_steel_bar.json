{
  "multipart": [
    {
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "xkdeco:block/hollow_steel_bar_noside",
        "y": 270
      }
    }
  ]
}